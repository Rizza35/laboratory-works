#include <stdio.h>

int main() {
    int numbers[10];  // Массив из 10 целых чисел
    int sum_even = 0; // Сумма четных чисел
    int sum_odd = 0;  // Сумма нечетных чисел

    // Ввод 10 чисел с клавиатуры
    printf("Введите 10 целых чисел:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &numbers[i]);
    }

    // Подсчет сумм четных и нечетных чисел
    for (int i = 0; i < 10; i++) {
        if (numbers[i] % 2 == 0) {
            sum_even += numbers[i];
        } else {
            sum_odd += numbers[i];
        }
    }

    // Вывод результатов
    printf("Сумма четных чисел: %d\n", sum_even);
    printf("Сумма нечетных чисел: %d\n", sum_odd);

    // Определение и вывод наибольшей суммы
    if (sum_even > sum_odd) {
        printf("Наибольшая сумма: %d\n", sum_even);
    } else {
        printf("Наибольшая сумма: %d\n", sum_odd);
    }

    return 0;
}
